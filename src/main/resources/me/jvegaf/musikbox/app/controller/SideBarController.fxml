<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox stylesheets="@../../../../../styles/dark.css" xmlns="http://javafx.com/javafx/11.0.2"
      fx:controller="me.jvegaf.musikbox.app.controller.SideBarController" xmlns:fx="http://javafx.com/fxml/1">
    <Label styleClass="side-label" text="Library" VBox.vgrow="NEVER">
        <font>
            <Font size="13.0"/>
        </font>
    </Label>
    <ListView fx:id="libraryListView" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="90.0" VBox.vgrow="NEVER"/>
    <AnchorPane VBox.vgrow="NEVER">
        <Label styleClass="side-label" text="Playlists" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
               AnchorPane.topAnchor="0.0">
            <font>
                <Font size="13.0"/>
            </font>
        </Label>
        <HBox alignment="CENTER" layoutX="228.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="10.0"
              AnchorPane.topAnchor="0.0">
            <Button fx:id="addBtn" alignment="CENTER" mnemonicParsing="false" styleClass="add-playlist-btn"/>
        </HBox>
    </AnchorPane>
    <ListView fx:id="playlistListView" VBox.vgrow="ALWAYS"/>
</VBox>
