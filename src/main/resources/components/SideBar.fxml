<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root stylesheets="@../styles/dark.css" type="VBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <Label styleClass="side-label" text="Library" VBox.vgrow="NEVER">
      <font>
         <Font size="13.0" />
      </font>
   </Label>
   <ListView fx:id="libraryListView" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="90.0" VBox.vgrow="NEVER" />
   <Label styleClass="side-label" text="Playlists" VBox.vgrow="NEVER">
      <font>
         <Font size="13.0" />
      </font>
   </Label>
   <ListView fx:id="playlistListView" VBox.vgrow="ALWAYS" />
</fx:root>
