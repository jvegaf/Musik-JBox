<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1440.0" stylesheets="@../styles/dark.css"
      xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <fx:include fx:id="header" VBox.vgrow="NEVER" source="/components/Header.fxml"/>
    <HBox VBox.vgrow="ALWAYS">
        <fx:include fx:id="sidebar"
                    HBox.hgrow="NEVER" source="/components/SideBar.fxml"/>
        <fx:include fx:id="tracklist" HBox.hgrow="ALWAYS" source="/components/Tracklist.fxml"/>
    </HBox>

    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
        <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
        </padding>
        <Label fx:id="leftStatusLabel" text="Left status" HBox.hgrow="ALWAYS"/>
        <Pane HBox.hgrow="ALWAYS"/>
        <Label fx:id="rightStatusLabel" text="Right status" HBox.hgrow="NEVER"/>
    </HBox>
</VBox>
