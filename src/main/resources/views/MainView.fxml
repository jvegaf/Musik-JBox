<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1440.0" stylesheets="@../styles/dark.css"
      xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <VBox fx:id="headerPane" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="70.0" VBox.vgrow="NEVER"/>
    <HBox VBox.vgrow="ALWAYS">
        <AnchorPane fx:id="sidePane" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0"
                    HBox.hgrow="NEVER"/>
        <AnchorPane fx:id="tracklistPane" HBox.hgrow="ALWAYS"/>
    </HBox>

    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
        <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
        </padding>
        <Label maxHeight="1.7976931348623157E308" text="Left status" HBox.hgrow="ALWAYS">
            <font>
                <Font size="11.0" fx:id="x3"/>
            </font>
            <textFill>
                <Color red="0.625" green="0.625" blue="0.625" fx:id="x4"/>
            </textFill>
        </Label>
        <Pane HBox.hgrow="ALWAYS"/>
        <Label font="$x3" text="Right status" textFill="$x4" HBox.hgrow="NEVER"/>
    </HBox>
</VBox>
